<ContentDialog
    x:Class="Petrolhead.Views.IncompleteAreaDialog"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:Petrolhead.Views"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    Title="PROCEED WITH CAUTION"
    PrimaryButtonText="Stop!!!"
    SecondaryButtonText="Continue anyway"
    PrimaryButtonClick="ContentDialog_PrimaryButtonClick"
    SecondaryButtonClick="ContentDialog_SecondaryButtonClick">

    <Grid>
        <TextBlock TextWrapping="Wrap">
             You are entering a part of Petrolhead that is currently incomplete and under active development. Continuing is inadvisable, and can result in data loss. <LineBreak /> Do you still want to continue?
            
        </TextBlock>
    </Grid>
</ContentDialog>
